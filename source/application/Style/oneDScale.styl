.OneDScale .XTicks {
	position: relative;
	left: 20%;
	width: 60%;
}

.OneDScale .SliderContainer {
	position: relative;
}

.OneDScale .SliderContainer input[type="range"] {
	position: relative;
	left: 20%;
	width: 60%;
}

.OneDScale .YTicks {
	position: absolute;
	padding: 0 10px;
	top: 50%;
	-webkit-transform: translateY(-50%);
	-moz-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	-o-transform: translateY(-50%);
	transform: translateY(-50%);
}

.OneDScale .XTicks .Tick {
	position: absolute;
}

.OneDScale .XTicks.X1 .Tick {
	top: 0;
	margin-top: 10px;
}

.OneDScale .XTicks.X2 .Tick {
	bottom: 0;
}

.OneDScale .XTicks .Tick:not(.MinPosition):not(.MaxPosition) {
	-webkit-transform: translateX(-50%);
	-moz-transform: translateX(-50%);
	-ms-transform: translateX(-50%);
	-o-transform: translateX(-50%);
	transform: translateX(-50%);
}

.OneDScale .XTicks .Tick.MinPosition {

}

.OneDScale .XTicks .Tick.MaxPosition {
	left: auto !important;
	right: 0;
}

.OneDScale .YTicks.Y1 {
	right: 80%;
}

.OneDScale .YTicks.Y2 {
	left: 80%;
}

.OneDScale .YTicks .Tick {
	display: block;
}

.OneDScale .YTicks.Y1 .Tick {
	text-align: right;
}

.OneDScale input[type="range"] {
	-webkit-appearance: none;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
	width: 100%;
	height: 20px;
	margin: 0;
	border: none;
	padding: 1px 2px;
	border-radius: 14px;
	background: #F0F0F0;
	box-shadow: inset 0 1px 0 0 #0d0e0f, inset 0 -1px 0 0 #3a3d42;
	-webkit-box-shadow: inset 0 1px 0 0 #0d0e0f, inset 0 -1px 0 0 #3a3d42;
	outline: none; /* no focus outline */
}

.OneDScale input[type="range"]::-moz-range-track {
	border: inherit;
	background: transparent;
}

.OneDScale input[type="range"]::-ms-track {
	border: inherit;
	color: transparent; /* don't draw vertical reference line */
	background: transparent;
}

.OneDScale input[type="range"]::-ms-fill-lower,
input[type="range"]::-ms-fill-upper {
	background: transparent;
}

.OneDScale input[type="range"]::-ms-tooltip {
	display: none;
}

.OneDScale input[type="range"]::-webkit-slider-thumb {
	-webkit-appearance: none;
	width: 20px;
	height: 18px;
	border: none;
	border-radius: 12px;
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #CCCCCC), color-stop(100%, #AAAAAA )); /* android <= 2.2 */
	background-image: -webkit-linear-gradient(top , #CCCCCC 0, #AAAAAA  100%); /* older mobile safari and android > 2.2 */
	background-image: linear-gradient(to bottom, #CCCCCC 0, #AAAAAA  100%); /* W3C */
}
.OneDScale input[type="range"]::-moz-range-thumb {
	width: 20px;
	height: 18px;
	border: none;
	border-radius: 12px;
	background-image: linear-gradient(to bottom, #CCCCCC 0, #AAAAAA  100%); /* W3C */
}

.OneDScale input[type="range"]::-ms-thumb {
	width: 20px;
	height: 18px;
	border-radius: 12px;
	border: 0;
	background-image: linear-gradient(to bottom, #CCCCCC 0, #AAAAAA  100%); /* W3C */
}

.OneDScale input[type="range"].NoValue::-webkit-slider-thumb {
	visibility: hidden;
}

.OneDScale input[type="range"].NoValue::-moz-range-thumb{
	visibility: hidden;
}

.OneDScale input[type="range"].NoValue::-ms-thumb {
	visibility: hidden;
}